{
    "name": "sarahman/guzzle-client-trait",
    "description": "This package is intended to handle guzzle client creation, its api calls & their corresponding responses.",
    "keywords": ["guzzle", "http", "client", "api", "request", "laravel", "lumen", "framework"],
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Syed Abidur Rahman",
            "email": "aabid048@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "^4.0 || ^5.0 || ^6.0 || ^7.0",
        "illuminate/support": ">=4.0"
    },
    "autoload": {
        "psr-4": {
            "Sarahman\\Traits\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
